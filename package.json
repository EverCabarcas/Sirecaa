{
  "name": "sirecaa-plataform",
  "version": "1.0.0",
  "engines": {
    "node": "8.1.4"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "del-cli public/js/app && webpack --config webpack.config.dev.js --progress --profile --watch",
    "build:prod": "del-cli public/js/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'public/js/app/**/*.js' 'public/js/app/**/*.js.map' '!public/js/app/bundle.js' '!public/js/app/*.chunk.js' 'assets/app/**/*.ngfactory.ts' 'assets/app/**/*.shim.ts'",
    "heroku-postbuild": "echo Skip build on Heroku"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "^2.6.9",
    "express": "^4.17.3",
    "hbs": "^3.1.1",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.12.4",
    "morgan": "^1.9.0",
    "promise": "^8.0.1",
    "reflect-metadata": "^0.1.3",
    "request": "^2.83.0",
    "requestify": "^0.2.5",
    "rxjs": "^5.5.2",
    "serve-favicon": "^2.4.5",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "^0.6.23"
  },
  "heroku-run-build-script": true
}
